{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/no-unresolved": ["warn"],
    "import/extensions": ["warn"],
    "import/no-named-default": ["warn"],
    "quotes": ["warn"],
    "comma-dangle": ["warn"]
  }
}
